{
  "name": "airdrop-csv-cleaner",
  "version": "1.0.0",
  "description": "Node tools for cleaning a CSV ready for an airdrop",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "csvToJSON": "npx run-func index.js csvToJSON raw.csv",
    "validateKeys": "npx run-func index.js removeOffCurveKeys",
    "bulkToken": "npx run-func index.js bulkTokenCSV 1000 soso1vCmdxwEZqU47M4NZ4MxZH19ppgqF1auG7dP3wz",
    "test:airdrop": "npx run-func airdrop.js exeAirdrop 0.00001 soso1vCmdxwEZqU47M4NZ4MxZH19ppgqF1auG7dP3wz ./data/test.json",
    "danger:airdrop": "npx run-func airdrop.js exeAirdrop 1000 soso1vCmdxwEZqU47M4NZ4MxZH19ppgqF1auG7dP3wz ./data/curved.json"
  },
  "author": "@harkl_",
  "license": "MIT",
  "dependencies": {
    "@moreavy/simple-csv-tools": "^1.0.1",
    "@solana/web3.js": "^1.29.2",
    "lodash": "^4.17.21",
    "objects-to-csv": "^1.3.6",
    "shelljs": "^0.8.4"
  }
}
